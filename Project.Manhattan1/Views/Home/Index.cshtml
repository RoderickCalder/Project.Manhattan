@{
    ViewBag.Title = "Home Page";
    @model   Project.Manhattan1.Models.MasterModel
  }
<!DOCTYPE html>
<head>
    <link rel="stylesheet" href="Site.css">
</head>
<script>
    $(".chatHistory").animate({ scrollTop: $(this).height() }, "slow");
</script>
<html>
<div class="sidebar-nav-fixed pull-right affix">
    <div class="userList">
        <ul id="UserList">
            @foreach (var u in Model.User) { 
            <li class="listItem">@Html.ActionLink(u.Name,"GetChat", new { id = u.Id, userName = u.Name }, null) </li>
            }
        </ul>
    </div>
</div>
<div class="messageBox">
    <div class="chatHistory">
        <table class="messageTable">
            @if (Model.Message != null)
            {
                foreach (var m in Model.Message)
                {
                    <tr>
                        <td>@m.UserTo</td>
                        <td>@m.Message</td>
                        <td>@m.DateTime</td>
                    </tr>

                }
            }
            
        </table>
    </div>
    <div class="sendMessage">
        <form enctype='application/json' align="center" action="/Home/SendChat" method="post">
            <textarea id="send-message" name='message' value='#' placeholder="Enter your message..." rows="4" cols="50"></textarea>
            <br>
            <button id="submit-button" type="submit" action="">Send Message</button>
        </form>
    </div>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <div class="carousel-inner center-block" role="listbox">
            <div class="item active">
                <img src="data:image/jpeg;base64,">
            </div>

            <div class="item center-block">
                <img src="data:image/jpeg;base64,">
            </div>

            <div class="item center-block">
                <img src="data:image/jpeg;base64,">
            </div>
            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</html>